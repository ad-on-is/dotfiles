[
  {
    "label": "PHP: Listen to Xdebug",
    "adapter": "Xdebug",
    "request": "launch",
    "pathMappings": {
      "/app": "$ZED_WORKTREE_ROOT",
    },
    "port": 9001,
  },
  {
    "label": "Debug Flutter App",
    "adapter": "Dart", // DO NOT EDIT
    "type": "flutter",
    "program": "lib/main.dart",
    "args": ["--web-port=9090"],
  },
  {
    "label": "Go (Delve)",
    "adapter": "Delve",
    "program": "$ZED_FILE",
    "request": "launch",
    "mode": "debug",
  },
]
